{
  "name": "continuous",
  "version": "0.1.3",
  "description": "Continuous integration and delivery experiments",
  "main": "build/main.js",
  "scripts": {
    "dev": "nodemon source/main.js --exec babel-node",
    "test": "jest",
    "build": "babel source -d build",
    "postversion": "git push && git push --tags",
    "start": "npm run build && node build/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonkuhrt/continuous.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jasonkuhrt/continuous/issues"
  },
  "homepage": "https://github.com/jasonkuhrt/continuous#readme",
  "dependencies": {
    "bluebird": "^3.4.6",
    "express": "^4.14.0",
    "ioredis": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-jest": "^17.0.2",
    "babel-preset-env": "0.0.9",
    "babel-preset-stage-3": "^6.17.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^17.0.3",
    "node-fetch": "^1.6.3",
    "nodemon": "^1.11.0"
  },
  "babel": {
    "presets": [
      "stage-3",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "setupFiles": [
      "TestSetup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "source/**/*.js"
    ]
  },
  "engines": {
    "node": ">=7.2.0 <8.0.0"
  }
}
